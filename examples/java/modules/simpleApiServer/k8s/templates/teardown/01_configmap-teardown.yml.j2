kind: ConfigMap
apiVersion: v1
metadata:
  name: "{{ app.simpleApiServer.name }}-teardown"
  namespace: "{{ app.simpleApiServer.namespace }}"
  labels:
    app.kubernetes.io/name: "{{ app.simpleApiServer.name }}-teardown"
    app.kubernetes.io/managed-by: "{{ app.simpleApiServer.manager }}"
    app.kubernetes.io/version: "{{ app.simpleApiServer.version }}"
    app.kubernetes.io/component: "{{ app.simpleApiServer.name }}"
    app.kubernetes.io/part-of: "{{ app.simpleApiServer.name }}"
data:
  standard: {{ app_k8s_objects }} #there should be a new line after this line with nothing on it, that means no spaces or tabs either.
